runtime: nodejs20
env: standard
instance_class: F1

# Environment variables - замените на ваши реальные значения!
# ВАЖНО: Не коммитьте реальные секреты в git!
# Скопируйте этот файл в app.yaml и заполните реальными значениями
env_variables:
  JWT_SECRET: 'your_secret_key_here'
  GEMINI_API_KEY: 'your_gemini_api_key_here'
  FIREBASE_SERVICE_ACCOUNT_KEY: '{"type":"service_account",...}'

